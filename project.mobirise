          "_customHTML": "<section mbr-id=\"_three_in_one_form\" class=\"mbr-section three_in_one_form\" plugins=\"singa4real-form-flatfile\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"8\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"3\">\n        <input type=\"color\" title=\"Background Color\" name=\"bgColor\" value=\"#bed3f9\">\n      <input type=\"checkbox\" title=\"Show logo\" name=\"logo\" checked>\n      <select title=\"Form Type\" name=\"ftype\">\n            <option value=\"1\">Registrate</option>\n            <option value=\"2\" selected>Ingresa</option>\n            <option value=\"3\">Olvidaste la contraseña</option>\n        </select>\n\n      <hr><header>Ajuste de formulario</header>\n      <input type=\"checkbox\" title=\"Show Username\" name=\"username\" condition=\"ftype == 1\" checked>\n      <input type=\"checkbox\" title=\"Show Fullname\" name=\"fullname\" checked condition=\"ftype == 1\">\n      <input type=\"checkbox\" title=\"Show Email\" name=\"email\" condition=\"ftype == 1\" checked>\n      <input type=\"checkbox\" title=\"Show Gender\" name=\"gender\" condition=\"ftype == 1\" checked>\n      <input type=\"checkbox\" title=\"Show Phone\" name=\"phone\" condition=\"ftype == 1\" checked>\n      \n      <select title=\"Login Using\" name=\"loginusing\" condition=\"ftype == 2\">\n            <option value=\"1\" selected>Nombre</option>\n            <option value=\"2\">Correo</option>ombre\n            <option value=\"2\">Correo</option>\n        </select>\n    <!-- End block parameters -->\n    </mbr-parameters> \n\n    <!-- Navbar-->\n<header class=\"header\" mbr-if=\"logo\">\n    <nav class=\"navbar navbar-expand-lg navbar-light py-3\">\n        <div class=\"container\">\n            <!-- Navbar Brand -->\n            <a href=\"#\" class=\"navbar-brand\">\n               <img src=\"@PROJECT_PATH@/assets/images/178397637-3604450233113923-4475258095376129709-n-107x107.png\" alt=\"logo\" width=\"150\">\n            </a>\n        </div>\n    </nav>\n</header>\n\n\n<div class=\"container\">\n    <div class=\"row py-5 mt-4 align-items-center\">\n        <!-- For Demo Purpose -->\n        <div class=\"col-lg-6 d-xl-block d-lg-block d-none pr-lg-5 mb-5 mb-md-0\">\n            <img src=\"https://res.cloudinary.com/mhmd/image/upload/v1569543678/form_d9sh6m.svg\" alt class=\"img-fluid mb-3\">\n            <h5>Ingresa<br><br></h3>\n            <p class=\"font-italic text-muted mb-0\">Esta es la plataforma interactiva de aprendizaje de Inglés que estabas esperando, no te quedes atrás.<br></p>\n            <p class=\"font-italic text-muted\">Síguenos en Facebook <a href=\"https://web.facebook.com/InglesAI\" class=\"text-primary\" target=\"_blank\">EnglishAI</a> para aprender inglés Hoy.</p>\n        </div>\n      \n        <!-- Registeration Form -->\n        <div class=\"col-md-12 col-lg-6 ml-auto\" mbr-if=\"ftype == 1\">\n            \n              <form action=\"#\" method=\"post\" class=\"mbr-form usermanager_form\">\n              <div class=\"row\">\n                        <div hidden=\"hidden\" class=\"alert alert-success col-12\"></div>\n                        <div hidden=\"hidden\" class=\"alert alert-danger col-12\"></div>\n\t\t\t\t\t\t<input type=\"hidden\" name=\"form_name\" value=\"signupform\">\n                    </div>\n                <div class=\"row\">\n\n                    <!-- Full Name -->\n                    <div class=\"input-group col-lg-12 mb-4\" mbr-if=\"fullname\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-user text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"fullame\" type=\"text\" name=\"fullname\" placeholder=\"Nombre completo\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"Fullname\" required=\"required\">\n                    </div>\n                    <!-- Phone Number -->\n                    <div class=\"input-group col-lg-6 mb-4\" mbr-if=\"phone\" mbr-class=\"{'col-lg-12': !gender}\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-phone-square text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"phoneNumber\" type=\"tel\" name=\"phone\" placeholder=\"Numero de telefono\" class=\"form-control bg-white border-md border-left-0 pl-3\" data-form-field=\"Phone\" required=\"required\">\n                    </div>\n\n\n                    <!-- Gender -->\n                    <div class=\"input-group col-lg-6 mb-4\" mbr-if=\"gender\" mbr-class=\"{'col-lg-12': !phone}\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-black-tie text-muted fa-briefcase fas\"></i>\n                            </span>\n                        </div>\n                        <select id=\"gender\" name=\"gender\" class=\"form-control custom-select bg-white border-left-0 border-md\" data-form-field=\"Gender\" required=\"required\">\n                            <option value>Escoge tu Sexo</option>\n                            <option value=\"Male\">Masculino</option>\n                            <option value=\"Female\">Femenino</option>\n                        </select>\n                    </div>\n                    <!-- Username -->\n                    <div class=\"input-group col-lg-6 mb-4\" mbr-if=\"username\" mbr-class=\"{'col-lg-12': !email}\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-user text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"username\" type=\"text\" name=\"username\" placeholder=\"Nombre\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"username\" required=\"required\">\n                    </div>\n                    <!-- Email Address -->\n                    <div class=\"input-group col-lg-6 mb-4\" mbr-if=\"email\" mbr-class=\"{'col-lg-12': !username}\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-envelope text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"email\" type=\"email\" name=\"email\" placeholder=\"Correo electrónico\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"Email\" required=\"required\">\n                    </div>\n                    <!-- Password -->\n                    <div class=\"input-group col-lg-6 mb-4\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-lock text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"password\" type=\"password\" name=\"password\" placeholder=\"Contraseña\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"Password\" required=\"required\">\n                    </div>\n\n                    <!-- Password Confirmation -->\n                    <div class=\"input-group col-lg-6 mb-4\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-lock text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"passwordConfirmation\" type=\"password\" name=\"confirmpassword\" placeholder=\"Confirma tu contraseña\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"Confirm Password\" required=\"required\">\n                    </div>\n\n                    <!-- Submit Button -->\n                    <div class=\"form-group col-lg-12 mx-auto mb-0\" mbr-buttons=\"true\" data-toolbar=\"-mbrBtnMove,-mbrBtnLink,-mbrBtnAdd\">\n                            <button type=\"submit\" class=\"btn btn-primary btn-block font-weight-bold py-2\">Crea tu cuenta</button>\n                        </div>\n\n                    <!-- Divider Text -->\n                    <div class=\"form-group col-lg-12 mx-auto d-flex align-items-center my-4\" mbr-if=\"Socialsignup\">\n                        <div class=\"border-bottom w-100 ml-5\" mbr-if=\"Socialsignup\"></div>\n                        <span class=\"px-2 small text-muted font-weight-bold text-muted\" mbr-if=\"Socialsignup\">OR</span>\n                        <div class=\"border-bottom w-100 mr-5\" mbr-if=\"Socialsignup\"></div>\n                    </div>\n\n                    <!-- Social Login -->\n                    <div class=\"form-group col-lg-12 mx-auto\" mbr-if=\"Socialsignup\" mbr-buttons=\"true\" data-toolbar=\"-mbrBtnMove,-mbrBtnLink,-mbrBtnAdd\"><a href=\"#\" class=\"btn btn-primary btn-block py-2 btn-facebook\" data-app-placeholder=\"Type Text\">\n                            <i class=\"fa fa-facebook-f mr-2 fab\"></i>\n                            <span class=\"font-weight-bold\">Continua con Facebook</span>\n                        </a>\n                        <a href=\"#\" class=\"btn btn-primary btn-block py-2 btn-twitter\" data-app-placeholder=\"Type Text\">\n                            <i class=\"fa fa-twitter mr-2\"></i>\n                            <span class=\"font-weight-bold\">Continua con Twitter</span>\n                        </a></div>\n\n                    <!-- Already Registered -->\n                    <div class=\"text-center w-100\">\n                        <p class=\"text-muted font-weight-bold\">¿Ya estás registrado? <a href=\"login.html\" class=\"text-primary ml-2\">Ingresa</a></p>\n                    </div>\n\n                </div>\n            </form>\n        </div>\n      \n      \n      \n      \n\n        <div class=\"col-md-7 col-lg-6 ml-auto\" mbr-if=\"ftype == 2\">\n            <form action=\"#\" method=\"post\" class=\"mbr-form usermanager_form\">\n              <div class=\"row\">\n                        <div hidden=\"hidden\" class=\"alert alert-success col-12\"></div>\n                        <div hidden=\"hidden\" class=\"alert alert-danger col-12\"></div>\n                        <input type=\"hidden\" name=\"form_name\" value=\"loginform\">\n                        \n                    </div>\n                <div class=\"row\">\n                   <!-- Email -->\n                    <div class=\"input-group col-lg-12 mb-4\" mbr-if=\"loginusing == 2\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-envelope text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"email-login\" type=\"text\" name=\"email\" placeholder=\"Tu correo\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"email\" required=\"required\">\n                    </div>\n                  <!-- Username -->\n                  <div class=\"input-group col-lg-12 mb-4\" mbr-if=\"loginusing == 1\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-user text-muted fas fa-fw\"></i>\n                            </span>\n                        </div>\n                        <input id=\"username-login\" type=\"text\" name=\"username\" placeholder=\"Nombre\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"username\" required=\"required\">\n                    </div>\n\n                    <!-- Password -->\n                    <div class=\"input-group col-lg-12 mb-4\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-lock text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"password2\" type=\"password\" name=\"password\" placeholder=\"Contraseña\" class=\"form-control bg-white border-left-0 border-md\" required=\"required\">\n                    </div>\n\n                    <!-- Submit Button -->\n                    <div class=\"form-group col-lg-12 mx-auto mb-0\" mbr-buttons=\"true\" data-toolbar=\"-mbrBtnMove,-mbrBtnLink,-mbrBtnAdd\"><button type=\"submit\" class=\"btn btn-primary btn-block font-weight-bold py-2\">Ingresa a tu cuenta</button></div>\n\n                    <!-- Divider Text -->\n                    <div class=\"form-group col-lg-12 mx-auto d-flex align-items-center my-4\" mbr-if=\"Socialsignup\">\n                        <div class=\"border-bottom w-100 ml-5\" mbr-if=\"Socialsignup\"></div>\n                        <span class=\"px-2 small text-muted font-weight-bold text-muted\" mbr-if=\"Socialsignup\">OR</span>\n                        <div class=\"border-bottom w-100 mr-5\" mbr-if=\"Socialsignup\"></div>\n                    </div>\n\n                    <!-- Social Login -->\n                    <div class=\"form-group col-lg-12 mx-auto\" mbr-if=\"Socialsignup\" mbr-buttons=\"true\" data-toolbar=\"-mbrBtnMove,-mbrBtnLink,-mbrBtnAdd\">\n                        <a href=\"#\" class=\"btn btn-primary btn-block py-2 btn-facebook\">\n                            <i mbr-icon class=\"fa fa-facebook-f mr-2\"></i>\n                            <span class=\"font-weight-bold\">Continua con Facebook</span>\n                        </a>\n                        <a href=\"#\" class=\"btn btn-primary btn-block py-2 btn-twitter\">\n                            <i mbr-icon class=\"fa fa-twitter mr-2\"></i>\n                            <span class=\"font-weight-bold\">Continua con Twitter</span>\n                        </a>\n                    </div>\n\n                    <!-- Already Registered -->\n                    <div class=\"text-center w-100\">\n                        <p class=\"text-muted font-weight-bold\">¿Olvidaste tu contraseña? <a href=\"./reset_password.html\" class=\"text-primary ml-2\">Hazte otra contraseña</a></p>\n                    </div>\n\n                </div>\n            </form>\n        </div>\n      \n      \n      \n        <div class=\"col-md-7 col-lg-6 ml-auto\" mbr-if=\"ftype == 3\">\n            <form action=\"#\" method=\"post\" class=\"mbr-form usermanager_form\">\n              <div class=\"row\">\n                        <div hidden=\"hidden\" class=\"alert alert-success col-12\"></div>\n                        <div hidden=\"hidden\" class=\"alert alert-danger col-12\"></div>\n                         <input type=\"hidden\" name=\"form_name\" value=\"forgotpassword\">\n                    </div>\n                <div class=\"row\">\n\n                    <!-- Email Address -->\n                    <div class=\"input-group col-lg-12 mb-4\" mbr-if=\"resetusing == 2\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-envelope text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"email-reset\" type=\"text\" name=\"email\" placeholder=\"Your email\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"email\" required=\"required\">\n                    </div>\n                  <!-- Username -->\n                  <div class=\"input-group col-lg-12 mb-4\" mbr-if=\"resetusing == 1\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-user text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"username-reset\" type=\"text\" name=\"username\" placeholder=\"Username\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"username\" required=\"required\">\n                    </div>\n\n                    <!-- Submit Button -->\n                    <div class=\"form-group col-lg-12 mx-auto mb-0\" mbr-buttons=\"true\" data-toolbar=\"-mbrBtnMove,-mbrBtnLink,-mbrBtnAdd\">\n                      <button type=\"submit\" class=\"btn btn-primary btn-block font-weight-bold py-2\">Hazte otra contraseña</button>\n              \n                    </div>\n                    <!-- Already Registered -->\n                    <div class=\"text-center w-100\">\n                        <p class=\"text-muted font-weight-bold\">Recuerda mi contraseña <a href=\"./login.html\" class=\"text-primary ml-2\">Ingresa</a></p>\n                    </div>\n\n                </div>\n            </form>\n        </div>\n      \n    </div>\n</div>\n   \n    \n</section>",
